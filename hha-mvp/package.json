{
  "name": "hha-mvp",
  "version": "1.0.0",
  "description": "Home Health Automation MVP - Monorepo",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter portal dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "seed": "pnpm --filter @hha-mvp/testing seed",
    "demo": "tsx scripts/demo.ts"
  },
  "keywords": [
    "health",
    "automation",
    "home-health",
    "monorepo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/form-data": "^2.5.2",
    "@types/node": "^22.10.5",
    "@types/node-fetch": "^2.6.11",
    "form-data": "^4.0.1",
    "node-fetch": "^2.7.0",
    "prettier": "^3.2.5",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8.0.0"
  }
}

