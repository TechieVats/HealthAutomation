          "bodyContentType": "multipart-form-data"
        },
        "sendBody": true,
        "specifyBody": "json",
        "jsonBody": "={}",
        "sendBinaryData": true,
        "binaryPropertyName": "file",
        "fileName": "={{ $json.name }}"
      },
      "id": "upload-to-api",
      "name": "Upload to API",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [650, 300],
      "notes": "Uploads PDF to /api/referrals/upload endpoint"
    },
    {
      "parameters": {},
      "id": "log-result",
      "name": "Log Result",
      "type": "n8n-nodes-base.noOp",
      "typeVersion": 1,
      "position": [850, 300]
    }
  ],
  "connections": {
    "Watch Referrals Folder": {
