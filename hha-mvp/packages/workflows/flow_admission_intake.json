      "typeVersion": 1,
      "position": [450, 300],
      "notes": "In production, this would call Tesseract OCR service"
    },
    {
      "parameters": {
        "url": "={{ $env.NEXT_PUBLIC_BASE_URL || 'http://localhost:3000' }}/api/referrals/upload",
